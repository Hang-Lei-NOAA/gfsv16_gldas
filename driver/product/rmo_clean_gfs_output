mkdir -m 775 -p  /gpfs/dell2/ptmp/Boi.Vuong/com

chmod  -R 775 /gpfs/dell2/ptmp/Boi.Vuong/output /gpfs/dell2/ptmp/Boi.Vuong/com
 
cd /gpfs/dell2/ptmp/Boi.Vuong/output
